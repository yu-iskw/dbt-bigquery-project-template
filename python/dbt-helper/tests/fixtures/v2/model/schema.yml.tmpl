{% import "macros.jinja" as macros -%}

{%- set documentation = '{{{{ doc("{}") }}}}'.format(model_reference_id) -%}
---
# It was generated by dbt-helper=={{ dbt_helper_version }}.
version: 2

models:
  - name: {{ model_reference_id }}
    description: |
      {{ documentation }}
    meta:
      owner: "{{- owner -}}"
      contains_pii: "false | true"
    {% if labels | length > 0 %}
    {% for k, v in labels.items() -%}
      {{- k -}}: "{{- v -}}"
    {% endfor -%}
    {% endif %}
    columns:
      - name: DUMMY_COLUMN_NAME
        description: "DESCRIBE THE COLUMN"
        tests:
          # Please modify the test cases
          - unique
          - not_null
          - accepted_values:
              values: ["a", "b", "c"]